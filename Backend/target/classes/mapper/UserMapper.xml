<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scholarship.mapper.UserMapper">
    <select id="findUser" resultType="com.scholarship.entity.User">
        SELECT * FROM users WHERE username=#{username} AND password=#{password} and user_type =#{userType}
    </select>
</mapper>
